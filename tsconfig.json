{
	"extends": "./.svelte-kit/tsconfig.json",
    "compilerOptions": {
    "module": "es6",
    "target": "es2017",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "noImplicitAny": false,
    "strict": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "declaration": false,
    "jsx": "preserve",
    "skipLibCheck": true
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules/*", "__sapper__/*", "public/*"]
}
